            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9RqMNeDzEeSdu9_RjyRTQA" name="getLastPlaces" specification="_8w_IEODzEeSdu9_RjyRTQA">
              <ownedParameter xmi:id="_9RqMNuDzEeSdu9_RjyRTQA" type="_8xuH4ODzEeSdu9_RjyRTQA" isOrdered="true" direction="return">
                <upperValue xmi:type="uml:LiteralUnlimitedNatural" xmi:id="_9RqMN-DzEeSdu9_RjyRTQA" value="*"/>
              </ownedParameter>
              <language>java</language>
              <body>List&lt;Place> List = new ArrayList&lt;Place>();&#xD;
		// Select All Query&#xD;
		String selectQuery = &quot;Select * FROM &quot; + Database.TABLE_LASTLIST;&#xD;
&#xD;
		Cursor cursor = database.rawQuery(selectQuery, null);&#xD;
		// looping through all rows and adding to list&#xD;
		if (cursor.moveToFirst()) {&#xD;
			do {&#xD;
				Place p = new Place();&#xD;
				p.setName(cursor.getString(1));&#xD;
				p.setID(cursor.getInt(2)); // INFO ID of Place = PrimeID on&#xD;
											// Server&#xD;
				// Adding to list&#xD;
				List.add(p);&#xD;
&#xD;
			} while (cursor.moveToNext());&#xD;
			// List.toString();&#xD;
		}&#xD;
		// return list&#xD;
		return List;&#xD;
	</body>
            </ownedBehavior>
