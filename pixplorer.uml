            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9RyvEODzEeSdu9_RjyRTQA" name="getHighResImage" specification="_8yJlsODzEeSdu9_RjyRTQA">
              <ownedParameter xmi:id="_9RyvEeDzEeSdu9_RjyRTQA" name="minWidth">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <ownedParameter xmi:id="_9RyvEuDzEeSdu9_RjyRTQA" name="minHeight">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/JavaPrimitiveTypes.library.uml#int"/>
              </ownedParameter>
              <ownedParameter xmi:id="_9RyvE-DzEeSdu9_RjyRTQA" direction="return">
                <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
              </ownedParameter>
              <language>java</language>
              <body>String url = image_src + &quot;?fm=png&quot;;

     if (minWidth > 0 &amp;&amp; minHeight > 0) {
         float phoneRatio = (1.0f * minWidth) / minHeight;
         if (phoneRatio &lt; getRatio()) {
             if (minHeight &lt; 1080) {
                 //url = url + &quot;&amp;h=&quot; + minHeight;
                 url = url + &quot;&amp;h=&quot; + 1080;
             }
         } else {
             if (minWidth &lt; 1920) {
                 //url = url + &quot;&amp;w=&quot; + minWidth;
                 url = url + &quot;&amp;w=&quot; + 1920;
             }
         }
     }

     return url;
 </body>
            </ownedBehavior>
