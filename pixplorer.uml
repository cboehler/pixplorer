            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_9RsBZeDzEeSdu9_RjyRTQA" name="getAllMYplaces" specification="_8xEnoODzEeSdu9_RjyRTQA">
              <ownedParameter xmi:id="_9RsBZuDzEeSdu9_RjyRTQA" type="_8xuH4ODzEeSdu9_RjyRTQA" isOrdered="true" direction="return">
