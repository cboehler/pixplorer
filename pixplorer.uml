              <language>java</language>
              <body>resultList.clear();
		int count = 0;
		for (int x = 0; x &lt; searchables.size(); x++) {
			if (searchables.get(x).title.toLowerCase().startsWith(
					getSearchText().toLowerCase())
					&amp;&amp; count &lt; 5) {
				addResult(searchables.get(x));
				count++;
