		search.addTextChangedListener(new TextWatcher() {

			@Override
			public void afterTextChanged(Editable s) {
				if (s.length() > 0) {
					isMic = false;
					mic.setImageDrawable(context.getResources().getDrawable(
							R.drawable.ic_clear));
					updateResults();
				} else {
					isMic = true;
